    "ember-addon",
    "i18n",
    "internationalization",
    "translation",
    "crowdin",
    "ember",
    "ember-i18n"
  ],
  "license": "MIT",
  "author": "Xavier Cambar <xcambar@gmail.com>",
  "directories": {
